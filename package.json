        "helmet": "^4.6.0",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.7",
        "redis": "^3.1.2",
        "supertest": "^6.1.3",
        "ts-node": "^9.1.1",
        "tslib": "~2.2.0",
        "web3": "^1.3.5",
        "winston": "^3.3.3",
        "yargs": "^15.4.1"
