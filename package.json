        "eslint": "~7.24.0",
        "eslint-config-prettier": "~8.1.0",
        "eslint-plugin-jest": "~24.3.5",
        "jest": "~26.6.3",
        "prettier": "~2.2.1",
        "rimraf": "~3.0.2",
        "ts-jest": "~26.5.5",
        "tsutils": "~3.21.0",
        "typescript": "~4.2.4"
